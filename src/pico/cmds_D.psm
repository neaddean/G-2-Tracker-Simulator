;;; | name         | s0 | s1 | s2 | s3 | s4 | s5 | s6 | s7 | s8 | s9 | sA | sB |
;;; | CMD_dac      | x  |    |    |    |    |    |    |    |    |    |    |    |


CMD_dac:
	LOAD 	s0, CLI_WORD_1
	FETCH 	SPI_LOW, (s0)

	ADD 	s0, 01
	FETCH	SPI_HIGH, (s0)

	CALL	send_spi

	RETURN
